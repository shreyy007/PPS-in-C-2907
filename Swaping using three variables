#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter the number 1\t");
    scanf("%d",&a);
    
    printf("Enter the number 2\t");
    scanf("%d",&b);
    
    printf("Before swaping:a=%d,b=%d\n",a,b);
    
    c=a;
    a=b;
    b=c;
    
    printf("After swaping:a=%d,b=%d\n",a,b);
   return 0;
   
